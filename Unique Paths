https://leetcode.com/problems/unique-paths/submissions/1844380291

#62


class Solution {
  
    int dp(int m, int n ,int [][] dp) {
        if (m == 1 || n == 1) {
            return 1;
        }
        if(dp[m][n]!=0) return dp[m][n] ;

        
        return dp[m][n]=dp(m - 1, n,dp) + dp(m, n - 1,dp);   
    }

    public int uniquePaths(int m, int n) {
        int [] [] dp =new int[m+1][n+1];
        int ans = dp(m, n, dp);
        return ans;
    }
}
